{
  "log": {
    "access": "/dev/null",
    "error": "/dev/null",
    "loglevel":"none"
  },
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": $VLTCPPORT,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "email": "vless-tcp",
            "flow": "",
            "id": "uuid",
            "password": ""
          }
        ],
        "decryption": "none",
        "encryption": "none",
        "testseed": [
          900,
          500,
          900,
          256
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "none",
        "tcpSettings": {
          "acceptProxyProtocol": false,
          "header": {
            "type": "none"
          }
        }
      },
      "sniffing": {
        "enabled": false,
        "destOverride": [
          "http",
          "tls",
          "quic",
          "fakedns"
        ]
      }
    },
    {
      "listen": "0.0.0.0",
      "port": $VLWSPORT,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "email": "vless-ws",
            "flow": "",
            "id": "uuid",
            "password": ""
          }
        ],
        "decryption": "none",
        "encryption": "none",
        "testseed": [
          900,
          500,
          900,
          256
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "acceptProxyProtocol": false,
          "headers": {},
          "heartbeatPeriod": 0,
          "host": "",
          "path": "/"
        }
      },
      "sniffing": {
        "enabled": false,
        "destOverride": [
          "http",
          "tls",
          "quic",
          "fakedns"
        ]
      }
    },
    {
      "listen": "0.0.0.0",
      "port": $VLXHTTPPORT,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "email": "vless-xhttp",
            "flow": "",
            "id": "uuid",
            "password": ""
          }
        ],
        "decryption": "none",
        "encryption": "none",
        "testseed": [
          900,
          500,
          900,
          256
        ]
      },
      "streamSettings": {
        "network": "xhttp",
        "security": "none",
        "xhttpSettings": {
          "headers": {},
          "host": "",
          "mode": "auto",
          "noSSEHeader": false,
          "path": "/",
          "scMaxBufferedPosts": 30,
          "scMaxEachPostBytes": "1000000",
          "scStreamUpServerSecs": "20-80",
          "xPaddingBytes": "100-1000"
        }
      },
      "sniffing": {
        "enabled": false,
        "destOverride": [
          "http",
          "tls",
          "quic",
          "fakedns"
        ]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom"
    }
  ]
}
